--// Premium System Configuration
local PremiumConfig = {
    PastebinURL = "https://pastebin.com/raw/4fBfj4JA",  -- Replace with your raw pastebin link
}

--// Services
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

--// Create Premium Tab immediately (no delay)
local PremiumTab = Window:Tab({
    Title = "Premium",
    Icon = "star",
    IconColor = Color3.fromRGB(255, 215, 0),
    IconShape = "Square",
    Border = false,
})

--// Get Hardware ID
local function GetHWID()
    local success, hwid = pcall(function()
        return game:GetService("RbxAnalyticsService"):GetClientId()
    end)
    
    if success and hwid then
        print("HWID obtained:", hwid)
        return hwid
    else
        local fallback = LocalPlayer.UserId .. "_fallback"
        warn("Failed to get HWID, using fallback:", fallback)
        return fallback
    end
end

--// Store HWID
local UserHWID = GetHWID()

--// Check if user has premium access via HWID whitelist
local function HasPremiumAccess(player)
    local success, result = pcall(function()
        local whitelist = game:HttpGet(PremiumConfig.PastebinURL)
        
        for hwid in string.gmatch(whitelist, "[^\r\n]+") do
            hwid = string.gsub(hwid, "%s+", "")
            if hwid == UserHWID then
                return true
            end
        end
        return false
    end)
    
    if success then
        print("Premium check result:", result)
        return result
    else
        warn("Failed to check whitelist")
        return false
    end
end

--// Get current user's premium status
local IsPremium = HasPremiumAccess(LocalPlayer)

local PremiumFeature1 = PremiumTab:Section({
    Title = "Avatar Items",
    Desc = "Client Sided Avatar Items",
    Icon = "star",
    Box = true,
    BoxBorder = true,
    TextXAlignment = "Center"
})

if IsPremium then
    PremiumFeature1:Button({ 
        Title = "Headless",
        Desc = "Gives you headless",
        Locked = false,
        Callback = function()

local player = game.Players.LocalPlayer

local function makeHeadInvisible(character)
    if not character then return end
    
    local head = character:FindFirstChild("Head")
    if head then
        -- Wait a bit to ensure all assets are loaded
        task.wait(0)
        
        -- Set the head's transparency to 1 to make it invisible
        head.Transparency = 1
        
        -- Remove the face decal if it exists
        for _, part in pairs(head:GetChildren()) do
            if part:IsA("Decal") then
                part:Destroy()
            end
        end
        
        -- Disable the head's collisions
        if head:IsA("BasePart") then
            head.CanCollide = false
        end
    end
end

-- Apply to current character if loaded
if player.Character then
    makeHeadInvisible(player.Character)
end

-- Ensure the effect is applied every time the player respawns
player.CharacterAdded:Connect(function(character)
    character:WaitForChild("Head") -- Ensure the head exists before modifying
    makeHeadInvisible(character)
end)
        end
    })
else
    PremiumFeature1:Button({ 
        Title = "Headless",
        Desc = "Premium required",
        Locked = true,
        Callback = function()
            -- Won't be called
        end
    })
end

if IsPremium then
    PremiumFeature1:Button({ 
        Title = "Korblox",
        Desc = "Gives you Korblox",
        Locked = false,
        Callback = function()
local player = game:GetService("Players").LocalPlayer

local function applyKorbloxLeg(char)
    repeat wait() until char:FindFirstChild("Right Leg")

    local rightLeg = char:FindFirstChild("Right Leg")
    if rightLeg then
        rightLeg.Transparency = 1

        local korbloxLeg = Instance.new("Part")
        korbloxLeg.Name = "KorbloxLeg"
        korbloxLeg.Size = Vector3.new(1, 1, 1)
        korbloxLeg.Position = rightLeg.Position
        korbloxLeg.Anchored = false
        korbloxLeg.CanCollide = false
        korbloxLeg.Parent = char

        local mesh = Instance.new("SpecialMesh")
        mesh.MeshId = "http://www.roblox.com/asset/?id=9598310133"
        mesh.TextureId = "http://roblox.com/asset/?id=902843398"
        mesh.Scale = Vector3.new(1, 1, 1)
        mesh.Parent = korbloxLeg

        local weld = Instance.new("Weld")
        weld.Part0 = rightLeg
        weld.Part1 = korbloxLeg
        weld.C0 = CFrame.new(0, 0.8, 0)
        weld.Parent = korbloxLeg
    end
end

if player.Character then
    applyKorbloxLeg(player.Character)
end

player.CharacterAdded:Connect(function(char)
    applyKorbloxLeg(char)
end)
        end
    })
else
    PremiumFeature1:Button({ 
        Title = "Korblox",
        Desc = "Premium required",
        Locked = true,
        Callback = function()
            -- Won't be called
        end
    })
end

if currentPlaceId == 9872472334 then
    local EvadePremium = PremiumTab:Section({
        Title = "Evade Features",
        Desc = "OP Features",
        Icon = "zap",
        Box = true,
        BoxBorder = true,
        TextXAlignment = "Center"
    })

    if IsPremium then
        EvadePremium:Button({
            Title = "Bounce",
            Desc = "Hold space when bouncing",
            Locked = false,
            Callback = function()
                local uis = game:GetService("UserInputService")
local rs = game:GetService("RunService")
local plr = game.Players.LocalPlayer

local bounceThreshold = -20
local initialBounceMultiplier = 1.2
local maxBounceMultiplier = 2.0
local bounceGrowthRate = 0.003
local crouchBoostMultiplier = 1.6
local upwardLimit = 100
local decayTime = 3
local crouching = false
local spaceHeld = false
local lastYVel = 0
local currentBounceMultiplier = initialBounceMultiplier
local lastBounceTime = tick()

uis.InputBegan:Connect(function(input, gpe)
    if gpe then return end
    if input.KeyCode == Enum.KeyCode.LeftControl then
        crouching = true
    elseif input.KeyCode == Enum.KeyCode.Space then
        spaceHeld = true
    end
end)

uis.InputEnded:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.LeftControl then
        crouching = false
    elseif input.KeyCode == Enum.KeyCode.Space then
        spaceHeld = false
    end
end)

rs.Stepped:Connect(function()
    local char = plr.Character
    if not char then return end
    local hrp = char:FindFirstChild("HumanoidRootPart")
    local hum = char:FindFirstChildWhichIsA("Humanoid")
    if not (hrp and hum) then return end
    local grounded = hum.FloorMaterial ~= Enum.Material.Air
    local yVel = hrp.Velocity.Y
    if grounded and (tick() - lastBounceTime > decayTime) then
        currentBounceMultiplier = initialBounceMultiplier
    end
    if grounded and lastYVel < bounceThreshold and spaceHeld then
        local bouncePower = -lastYVel * currentBounceMultiplier
        if crouching then
            bouncePower *= crouchBoostMultiplier
        end
        bouncePower = math.min(bouncePower, upwardLimit)
        hrp.Velocity = Vector3.new(hrp.Velocity.X, bouncePower, hrp.Velocity.Z)
        currentBounceMultiplier = math.min(currentBounceMultiplier + bounceGrowthRate, maxBounceMultiplier)
        lastBounceTime = tick()
    end
    lastYVel = yVel
end)
            end
        })
    else
        EvadePremium:Button({
            Title = "Bounce",
            Desc = "Premium required",
            Locked = true,
            Callback = function() end
        })
    end

if IsPremium then
    local toxicInfernoEnabled = false
    local toxicInfernoConnection = nil

    EvadePremium:Toggle({
        Title = "Toxic Inferno",
        Desc = "Client-sided cosmetic",
        Icon = "flame",
        Type = "Checkbox",
        Value = false,
        Callback = function(state)
            toxicInfernoEnabled = state
            local player = game.Players.LocalPlayer
            local ReplicatedStorage = game:GetService("ReplicatedStorage")
            
            local function removeToxicInferno(character)
                if not character then return end
                
                local head = character:FindFirstChild("Head")
                if not head then return end
                
                -- Remove all attachments and their children from head
                for _, child in pairs(head:GetChildren()) do
                    if child:IsA("Attachment") then
                        -- Check if it has ParticleEmitters (that's toxic inferno)
                        for _, particle in pairs(child:GetChildren()) do
                            if particle:IsA("ParticleEmitter") then
                                child:Destroy()
                                break
                            end
                        end
                    end
                end
                
                print("ðŸ—‘ï¸ Removed toxic inferno attachments")
            end
            
            local function equipToxicInferno(character)
                if not character then return end
                
                task.wait(0.5)
                
                local head = character:FindFirstChild("Head")
                
                if not head then 
                    warn("âš ï¸ Head not found")
                    return 
                end
                
                -- Remove existing first
                removeToxicInferno(character)
                
                if not toxicInfernoEnabled then
                    return
                end
                
                local toxicInferno = ReplicatedStorage.Items.Cosmetics:FindFirstChild("ToxicInferno")
                
                if not toxicInferno then
                    warn("âš ï¸ Toxic Inferno not found")
                    return
                end
                
                -- Clone Attachments and center them
                for _, descendant in pairs(toxicInferno:GetDescendants()) do
                    if descendant:IsA("Attachment") then
                        local cloned = descendant:Clone()
                        cloned.Parent = head
                        cloned.Position = Vector3.new(0, 0, 0)
                        cloned.CFrame = CFrame.new(0, 0, 0)
                    end
                end
                
                print("âœ… Toxic Inferno equipped!")
            end
            
            if state then
                -- Apply to current character
                if player.Character then
                    equipToxicInferno(player.Character)
                end
                
                -- Connect respawn handler
                if toxicInfernoConnection then
                    toxicInfernoConnection:Disconnect()
                end
                toxicInfernoConnection = player.CharacterAdded:Connect(equipToxicInferno)
                
                print("âœ… Toxic Inferno enabled")
            else
                -- Remove from current character IMMEDIATELY
                if player.Character then
                    removeToxicInferno(player.Character)
                end
                
                -- Disconnect respawn handler
                if toxicInfernoConnection then
                    toxicInfernoConnection:Disconnect()
                    toxicInfernoConnection = nil
                end
                
                print("âŒ Toxic Inferno disabled")
            end
        end
    })
else
    EvadePremium:Toggle({
        Title = "Toxic Inferno",
        Desc = "Premium required",
        Icon = "flame",
        Type = "Checkbox",
        Value = false,
        Locked = true,
        Callback = function() end
    })
end
end


if not IsPremium then
    print("Non-premium user detected, showing HWID info")
    print("UserHWID value:", UserHWID)
    
    PremiumTab:Paragraph({
        Title = "ðŸ”’ Premium Access Required",
        Desc = "Purchase in our discord and open a ticket and send your HWID to get access"
    })
    
    PremiumTab:Button({
        Title = "ðŸ“‹ Copy HWID",
        Desc = "Copy to clipboard",
        Locked = false,
        Callback = function()
            setclipboard(tostring(UserHWID))
            print("HWID copied: " .. tostring(UserHWID))
        end
    })
else
    print("Premium user detected")
end
