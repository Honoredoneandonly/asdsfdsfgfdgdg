local Version = "1.6.63"
local WindUI = loadstring(game:HttpGet(
    "https://github.com/Footagesus/WindUI/releases/download/" .. Version .. "/main.lua"
))()

--// Services
local RunService = game:GetService("RunService")
local Stats = game:GetService("Stats")

--// Theme
WindUI:AddTheme({
    Name = "Decay's Theme", -- theme name
    
    Icon = WindUI:Gradient({                                                      
        ["0"] = { Color = Color3.fromHex("#ffffff"), Transparency = 0 },            
        ["100"]   = { Color = Color3.fromHex("#050505"), Transparency = 0 },        
    }, {                                                                            
        Rotation = 0,                                                               
    }),

    Toggle = WindUI:Gradient({                                                      
        ["0"] = { Color = Color3.fromHex("#ffffff"), Transparency = 0 },            
        ["100"]   = { Color = Color3.fromHex("#000000"), Transparency = 0 },        
    }, {                                                                            
        Rotation = 0,                                                               
    }),
    Slider = WindUI:Gradient({                                                      
        ["0"] = { Color = Color3.fromHex("#615f5f"), Transparency = 0 },            
        ["100"]   = { Color = Color3.fromHex("#727272"), Transparency = 0 },        
    }, {                                                                            
        Rotation = 0,                                                               
    }),
})

local SupportedGames = {
    [10449761463] = "Game A",  -- replace with actual PlaceIds
    [9872472334] = "Game B",
    [213214124] = "Game C"
}

local currentPlaceId = game.PlaceId

-- Check if the game is supported
if not SupportedGames[currentPlaceId] then
    warn("Unsupported game! Script will not load.")
    return -- stop the script here; UI won't load
end

print("Supported game detected: " .. SupportedGames[currentPlaceId])

