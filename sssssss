-- FPS Tag
local FPSTag = Window:Tag({
    Title = "FPS: 0",
    Icon = "activity",
    Color = Color3.fromHex("#0073FF"),
    Radius = 13,
})

-- Ping Tag
local PingTag = Window:Tag({
    Title = "Ping: 0 ms",
    Icon = "wifi",
    Color = Color3.fromHex("#0073FF"),
    Radius = 13,
})

--// FPS + Ping Logic
local frames = 0
local lastTime = tick()

RunService.RenderStepped:Connect(function()
    frames += 1
    local now = tick()

    if now - lastTime >= 1 then
        -- FPS
        local fps = math.floor(frames / (now - lastTime))
        frames = 0
        lastTime = now

        FPSTag:SetTitle("FPS: " .. fps)

        if fps >= 60 then
            FPSTag:SetColor(Color3.fromRGB(0, 255, 0))
        elseif fps >= 30 then
            FPSTag:SetColor(Color3.fromRGB(255, 170, 0))
        else
            FPSTag:SetColor(Color3.fromRGB(255, 0, 0))
        end

        -- Ping
        local ping = math.floor(
            Stats.Network.ServerStatsItem["Data Ping"]:GetValue()
        )

        PingTag:SetTitle("Ping: " .. ping .. " ms")

        if ping <= 60 then
            PingTag:SetColor(Color3.fromRGB(0, 255, 0))
        elseif ping <= 120 then
            PingTag:SetColor(Color3.fromRGB(255, 170, 0))
        else
            PingTag:SetColor(Color3.fromRGB(255, 0, 0))
        end
    end
end)
    if currentPlaceId == 10449761463 then
